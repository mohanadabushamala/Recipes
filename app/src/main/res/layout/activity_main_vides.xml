<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainVides"
    android:background="@drawable/backgroand"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#E4252525"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/appCompatImageButton"
            android:layout_width="163dp"
            android:layout_height="119dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="96dp"
            android:src="@drawable/tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="TV"
            android:textColor="#FFFFFF"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/appCompatImageButton"
            app:layout_constraintStart_toStartOf="@+id/appCompatImageButton"
            app:layout_constraintTop_toBottomOf="@+id/appCompatImageButton" />


        <ImageView

            android:id="@+id/video_clup1"
            android:layout_width="163dp"
            android:layout_height="119dp"
            android:layout_marginStart="52dp"
            android:layout_marginLeft="52dp"
            android:src="@drawable/club"
            app:layout_constraintStart_toEndOf="@+id/appCompatImageButton"
            app:layout_constraintTop_toTopOf="@+id/appCompatImageButton" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="VIDEO CLUB"
            android:textColor="#FFFFFF"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/video_clup1"
            app:layout_constraintStart_toStartOf="@+id/video_clup1"
            app:layout_constraintTop_toBottomOf="@+id/video_clup1" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="163dp"
            android:layout_height="119dp"
            android:layout_marginTop="76dp"
            android:src="@drawable/seeting"
            app:layout_constraintStart_toStartOf="@+id/appCompatImageButton"
            app:layout_constraintTop_toBottomOf="@+id/appCompatImageButton" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="SEETINGS"
            android:textColor="#FFFFFF"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="163dp"
            android:layout_height="119dp"
            android:layout_marginTop="76dp"
            android:src="@drawable/radio"
            app:layout_constraintStart_toStartOf="@+id/video_clup1"
            app:layout_constraintTop_toBottomOf="@+id/video_clup1" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="163dp"
            android:layout_height="119dp"
            android:layout_marginTop="96dp"


            android:src="@drawable/audio_clup"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="RADIO"
            android:textColor="#FFFFFF"
            android:layout_marginTop="12dp"

            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView5"
            app:layout_constraintStart_toStartOf="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AUDIO CLUB"
            android:textColor="#FFFFFF"
            android:layout_marginTop="12dp"

            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView6"
            app:layout_constraintStart_toStartOf="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="163dp"
            android:layout_height="119dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="100dp"
            android:src="@drawable/playlist"
            app:layout_constraintStart_toStartOf="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PLAYLIST"
            android:layout_marginTop="12dp"

            android:textColor="#FFFFFF"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" />



    </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>